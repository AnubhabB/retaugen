{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:allow-home-read-recursive",
    "fs:allow-app-write-recursive",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:allow-save",
    "dialog:allow-open"
  ]
}